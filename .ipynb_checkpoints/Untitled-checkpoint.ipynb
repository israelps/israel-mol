    "def clean_newtonX_file(uri):\n",
    "    f = open(uri)\n",
    "    data = f.readlines()\n",
    "    f.close()\n",
    "    \n",
    "    import re\n",
    "    data = [line.strip() for line in data]\n",
    "    data = [re.sub(r' +', r' ',line)+'\\n' for line in data]\n",
    "    new_uri = uri.split('.')\n",
    "    new_uri[1] = '-filtered.dat'\n",
    "    new_uri = ''.join(new_uri)\n",
    "    f = open(new_uri, 'w')\n",
    "    f.writelines(data)\n",
    "    f.close()\n",
    "    print('file created at: %s'%new_uri)\n",
    "    \n",
    "    "
   ]
